{
  "expo": {
    "owner": "s-commerce",
    "name": "BTEX",
    "slug": "ecommerce",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "com.simeonecommerce.app",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.simeonecommerce.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets."
      }
    },
    "android": {
      "package": "com.simeonecommerce.app",
      "googleServicesFile": "./google-services.json",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "sentry-expo"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "SCTI",
            "project": "react-native",
            "authToken": "f9d6f325147e4517917b2ffc43a64a7dbb9fbd56911e4bb9be9494c4eaa33bc2"
          }
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "2fc6896c-bb29-4c37-9b14-8a8d84f91784"
      }
    }
  }
}
